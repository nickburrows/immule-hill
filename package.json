{
  "name": "static",
  "version": "1.0.0",
  "description": "This is a static template with no bundling",
  "main": "index.html",
  "scripts": {
    "start": "webpack serve",
    "dev": "webpack serve",
    "build": "webpack",
    "build:css": "npx tailwindcss -i ./style.css -o ./dist/tailwind.css",
    "css:watch": "npx tailwindcss -i ./style.css -o ./dist/tailwind.css --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codesandbox-app/static-template.git"
  },
  "keywords": [
    "static",
    "template",
    "codesandbox"
  ],
  "author": "Ives van Hoorne",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codesandbox-app/static-template/issues"
  },
  "homepage": "https://github.com/codesandbox-app/static-template#readme",
  "devDependencies": {
    "autoprefixer": "10.4.14",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "dotenv-webpack": "^8.0.1",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.29",
    "postcss-import": "15.1.0",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.1.3",
    "serve": "^11.2.0",
    "style-loader": "^3.3.3",
    "tailwindcss": "3.2.7",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "flowbite": "^1.8.1",
    "jquery": "^3.7.1",
    "material-symbols": "0.5.5"
  }
